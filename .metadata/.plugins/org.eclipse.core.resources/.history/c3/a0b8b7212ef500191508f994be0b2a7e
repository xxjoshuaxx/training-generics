package com.generics;

public class NonGenericExample01Demo {
	public static void main(String args[]) { 
	    NonGenericExample integerOb =new NonGenericExample(88);  

	    integerOb.showType(); 

	    
	    
	    int v = (Integer)integerOb.getobj(); //Cast is required 
	    System.out.println("value: " + v); 	 
	    System.out.println(); 
	 
	    NonGenericExample strOb = new NonGenericExample("Generics Test"); 
	    strOb.showType(); 
	    String str = strOb.getobj(); 
	    System.out.println("value: " + str);
	    
	    int v2 = (Integer)strOb.getobj();
	  } 
}
