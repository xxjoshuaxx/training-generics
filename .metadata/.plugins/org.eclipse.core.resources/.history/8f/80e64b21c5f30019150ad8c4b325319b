package com.generics;

class GenericExample01<T> { 
	  T ob; // declare an object of type T 
	   
	  // Pass the constructor a reference to  
	  // an object of type T. 
	  GenericExample01(T o) { 
	    ob = o; 
	  } 
	 
	  // Return ob. 
	  T getob() { 
	    return ob; 
	  } 
	 
	  // Show type of T. 
	  void showType() { 
	    System.out.println("Type of T is " + 
	                       ob.getClass().getName()); 
	  } 
	} 

